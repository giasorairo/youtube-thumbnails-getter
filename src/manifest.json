{
  "name": "youtube thumbnails getter",
  "author": "gia_sorairo",
  "version": "1.2.0",
  "manifest_version": 3,
  "description": "chrome extension to get youtube thumbnails",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup-dialog.html"
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "host_permissions": [
    "*://*/*"
  ]
}